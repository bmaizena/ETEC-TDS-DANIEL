Algoritmo "exN1maiorqueN2"

Var
   n1: inteiro
   n2: inteiro


Inicio
   escreva ("Digite o primeiro número: ")
   leia (n1)
   escreva ("Digite o seegundo número: ")
   leia (n2)
   se (n1 > n2) entao
      escreva ("o número ",n1, " é maior que o número ", n2)
   senao
      escreva ("o número ",n2, " é maior que o número ", n1)
   fimse


Fimalgoritmo